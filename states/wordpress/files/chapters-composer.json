{
    "name":"creativecommons/wordpress-chapters",
    "description":"Creative Commons Chapters WordPress Site via Composer",
    "authors": [
        {
            "name":"Creative Commons",
            "homepage":"https://github.com/creativecommons/sre-salt-prime"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type":"vcs",
            "url":"https://github.com/creativecommons/wp-theme-cc-chapter"
        }
    ],
    "config": {
        "vendor-dir":"wp-content/vendor"
    },
    "require": {
        "composer/installers":"1.6",
        "creativecommons/wp-theme-cc-chapter":"2019.4.3",
        "johnpbloch/wordpress":"5.0.3",
        "wpackagist-plugin/advanced-custom-fields":"5.7.9",
        "wpackagist-plugin/advanced-menu-widget":"0.4.1",
        "wpackagist-plugin/advanced-sidebar-menu":"7.4.6",
        "wpackagist-plugin/wordpress-seo":"9.6",
        "wpackagist-plugin/be-subpages-widget":"1.7",
        "wpackagist-plugin/google-analytics-for-wordpress":"7.4.2",
        "wpackagist-plugin/jetpack":"6.9",
        "wpackagist-plugin/redirection":"3.7.2",
        "wpackagist-plugin/ultimate-addons-for-gutenberg":"1.8.1",
        "wpackagist-plugin/widget-css-classes":"1.5.3",
        "wpackagist-plugin/wordpress-mu-domain-mapping":"0.5.5.1",
        "wpackagist-plugin/wp-latest-posts":"4.5.4",
        "wpackagist-plugin/wp-mail-smtp":"1.4.1"
    },
    "require-dev": {
        "wpackagist-plugin/debug-bar":"1.0",
        "wpackagist-plugin/debug-bar-actions-and-filters-addon":"1.5.4"
    },
    "extra": {
        "wordpress-install-dir":"wp"
    }
}
