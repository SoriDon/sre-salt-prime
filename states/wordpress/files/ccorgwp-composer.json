{
  "authors": [
    {
      "homepage": "https://github.com/creativecommons/sre-salt-prime",
      "name": "Creative Commons"
    }
  ],
  "config": {
    "vendor-dir": "wp-content/vendor"
  },
  "description": "Creative Commons Chapters WordPress Site via Composer",
  "extra": {
    "wordpress-install-dir": "wp"
  },
  "name": "creativecommons/wordpress-chapters",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "no-api": true,
      "type": "vcs",
      "url": "https://github.com/creativecommons/wp-theme-base"
    },
    {
      "no-api": true,
      "type": "vcs",
      "url": "https://github.com/creativecommons/wp-theme-creativecommons.org"
    },
    {
      "no-api": true,
      "type": "vcs",
      "url": "https://github.com/creativecommons/queulat"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://www.gravityforms.com"
        },
        "name": "junaidbhura/gravityforms",
        "require": {
          "johnpbloch/wordpress": "*",
          "junaidbhura/composer-wp-pro-plugins": "*"
        },
        "type": "wordpress-plugin",
        "version": "2.4.18"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://www.gravityforms.com"
        },
        "name": "junaidbhura/gravityformspaypal",
        "require": {
          "johnpbloch/wordpress": "*",
          "junaidbhura/composer-wp-pro-plugins": "*"
        },
        "type": "wordpress-plugin",
        "version": "3.3"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://www.gravityforms.com"
        },
        "name": "junaidbhura/gravityformsstripe",
        "require": {
          "johnpbloch/wordpress": "*",
          "junaidbhura/composer-wp-pro-plugins": "*"
        },
        "type": "wordpress-plugin",
        "version": "3.5"
      },
      "type": "package"
    }
  ],
  "require": {
    "composer/installers": "1.9",
    "creativecommons/wp-theme-creativecommons.org": "2020.07.1",
    "johnpbloch/wordpress": "5.4.1",
