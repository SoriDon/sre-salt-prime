    "wpackagist-plugin/simple-local-avatars": "2.1.1",
    "wpackagist-plugin/tablepress": "1.11",
    "wpackagist-plugin/vaultpress": "2.1.1",
    "wpackagist-plugin/w3-total-cache": "0.14.1",
