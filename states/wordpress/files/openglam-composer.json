{
    "name":"creativecommons/wordpress-openglam",
    "description":"Creative Commons OpenGLAM WordPress Site via Composer",
    "authors": [
        {
            "name":"Creative Commons",
            "homepage":"https://github.com/creativecommons/sre-salt-prime"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "config": {
        "vendor-dir":"wp-content/vendor"
    },
    "require": {
        "composer/installers":"^2.2",
        "johnpbloch/wordpress":"5.2.3",
        "wpackagist-plugin/akismet":"4.1.6",
        "wpackagist-plugin/better-click-to-tweet":"5.9.4",
        "wpackagist-plugin/classic-editor":"1.6",
        "wpackagist-plugin/cloudflare":"3.5.1",
        "wpackagist-plugin/google-calendar-events":"3.1.31",
        "wpackagist-plugin/iframe":"4.5",
        "wpackagist-plugin/jetpack":"8.8.2",
        "wpackagist-plugin/redirection":"4.8",
        "wpackagist-plugin/ssl-insecure-content-fixer":"2.7.2",
        "wpackagist-plugin/twitter-tools":"3.2.1",
        "wpackagist-theme/twentynineteen":"1.4",
        "wpackagist-plugin/wordfence": "7.4.11",
        "wpackagist-plugin/wordpress-seo": "15.1.1",
    },
    "require-dev": {
        "wpackagist-plugin/debug-bar":"1.0",
        "wpackagist-plugin/debug-bar-actions-and-filters-addon":"1.5.4"
    },
    "extra": {
        "wordpress-install-dir":"wp"
    }
}
